  "description": "Sway grammar for tree-sitter",
  "main": "bindings/node",
  "scripts": {
    "test": "tree-sitter test",
    "test-examples": "./scripts/parse-and-check.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FuelLabs/tree-sitter-sway.git"
  },
