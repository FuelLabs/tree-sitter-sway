    "parser"
  ],
  "author": "Fuel Labs <contact@fuel.sh>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/FuelLabs/tree-sitter-sway/issues"
  },
  "homepage": "https://github.com/FuelLabs/tree-sitter-sway#readme",
  "dependencies": {
    "nan": "^2.17.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.20.7"
  },
  "tree-sitter": [
    {
      "scope": "source.sway",
      "injection-regex": "sway",
